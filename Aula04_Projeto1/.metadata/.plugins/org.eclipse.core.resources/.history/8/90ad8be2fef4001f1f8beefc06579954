package negocio;

public class Colaborador {
	//Propriedades da Classe
	
	private int matricula = 0;
	private String nome = "";
	private double salario = 0;
	
	
	public Colaborador() {
		super();
	}
	
	
	public Colaborador(int matricula, String nome, double salario) {
		this.matricula = matricula;
		this.nome = nome;
		this.salario = salario;
	}
}
